{
  "name": "rexlabs/data-transfer-object",
  "type": "library",
  "description": "Data transfer objects with attributes",
  "keywords": [
    "rexlabs",
    "data-transfer-object"
  ],
  "homepage": "https://github.com/rexlabs/data-transfer-object",
  "license": "MIT",
  "authors": [
    {
      "name": "Lachlan Krautz",
      "email": "lachlan.krautz@rexsoftware.com.au",
      "homepage": "https://github.com/lachlankrautz",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "~7.2",
    "illuminate/support": "^5.6|^6.0",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": ">=8.0",
    "squizlabs/php_codesniffer": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "Rexlabs\\DataTransferObject\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Rexlabs\\DataTransferObject\\": "tests"
    }
  },
  "scripts": {
    "test": "phpunit",
    "check-style": "phpcs --colors src tests",
    "fix-style": "phpcbf --colors src tests"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  },
  "config": {
    "sort-packages": true,
    "platform": {
      "php": "7.2"
    }
  }
}
